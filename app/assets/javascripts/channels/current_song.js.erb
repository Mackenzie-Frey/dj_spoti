<% Party.all.each do |party| %>
App['current_song_' + `<%=party.identifier%>`] = App.cable.subscriptions.create({channel: 'CurrentSongChannel', party: `<%=party.identifier%>`}, {

  connected: function() {
    console.log('Got here. Connected');
  },
  disconnected: function() {
    console.log('disconnected');
  },

  received: function(data) {
    console.log("Receiving function with " + data.song);
    console.log(data.party);
    $('#currently-playing-song').html(data.song);

    // document.getElementById("#currently-playing-song").innerHTML = data.song;

 },
});
<% end %>
